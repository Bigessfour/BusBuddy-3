    <!-- Data grid style for Syncfusion SfDataGrid v30.2.5 -->
    <!-- Reference: https://help.syncfusion.com/wpf/datagrid/getting-started -->
    <!-- Note: Theming handled by SfSkinManager in App.xaml.cs, not individual property setters -->
    <Style x:Key="BusBuddySfDataGridStyle" TargetType="syncfusion:SfDataGrid">
        <Setter Property="Background" Value="{StaticResource BusBuddy.Brush.DataGrid.Background}" />
        <Setter Property="RowHeight" Value="28" />
        <Setter Property="HeaderRowHeight" Value="32" />
        <Setter Property="GridLinesVisibility" Value="Both" />
        <Setter Property="HeaderLinesVisibility" Value="Both" />
        <Setter Property="BorderBrush" Value="{StaticResource BusBuddy.Brush.DataGrid.Border}" />
        <Setter Property="BorderThickness" Value="1" />
        <Setter Property="AutoGenerateColumns" Value="False" />
        <Setter Property="AllowSorting" Value="True" />
        <Setter Property="AllowFiltering" Value="True" />
        <Setter Property="SelectionMode" Value="Single" />
        <Setter Property="ColumnSizer" Value="Star" />
    </Style>

